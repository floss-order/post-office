<table>
    <caption>
        <h1>ПОЛУЧАТЕЛИ</h1>
    </caption>
    <thead>
        <th>КОД ПОЛУЧАТЕЛЯ</th>
        <th>ФИО ПОЛУЧАТЕЛЯ</th>
        <th>АДРЕС ПОЛУЧАТЕЛЯ</th>
    </thead>
    <tbody>
        <% results.forEach(result => { %>
            <tr> 
                <td> <%= result['КОД_ПОЛУЧАТЕЛЯ'] %> </td>
                <td> <%= result['ФИО_ПОЛУЧАТЕЛЯ'] %> </td>
                <td> <%= result['АДРЕС_ПОЛУЧАТЕЛЯ'] %> </td>
            </tr>
         <% }) %> 
    </tbody>
</table>

<form action="/tables/recievers" method="post">
    <label for="query">Введите ваш запрос</label>
    <textarea name="query" id="qery" cols="30" rows="10"></textarea>
    <button type="submit">Отправить запрос</button>
</form>